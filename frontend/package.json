{
  "name": "helloworld-frontend",
  "version": "1.0.0",
  "description": "Helloworld前端Vue项目",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "install:deps": "node scripts/install.js",
    "start": "node scripts/start.js",
    "clean": "rm -rf node_modules dist .vite",
    "reinstall": "npm run clean && npm install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.5.0",
    "echarts": "^5.4.3",
    "element-plus": "^2.3.8",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-echarts": "^6.6.1",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.19.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.47.0",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^3.0.0",
    "sass": "^1.89.2",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  },
  "keywords": [
    "vue",
    "typescript",
    "vite",
    "frontend",
    "wechat",
    "baidu-map"
  ],
  "author": "Helloworld Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
